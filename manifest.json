{
    "name": "Distraction Tracker",
    "description": "Track the amount of time you spend on distracting websites.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {"128": "/images/get_started128.png"}
    },
    "icons": {"128": "/images/get_started128.png"}
}